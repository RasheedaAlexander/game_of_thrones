<h2>Characters <%= link_to "(+)", new_house_character_path(@house)%></h2>
<ul>
  <% @characters.each do |character| %>
  <h2><%=character.name%></h2
  <li>
    <%= link_to "#{character.name}", house_character_path(@house, @character) %>
  </li>
  <li>
    <%= link_to "Create Character", new_house_character_path(@house, @character) %>
  </li>
  <img src="<%= image_tag character.img_url %>">


  <% end %>
</ul>
